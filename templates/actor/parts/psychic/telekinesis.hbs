            <input type="radio" id="psychicThrowObjectResistance2" name="psychicThrowObjectResistance" class="radio-psychic-selected-throwobject-tr" data-cost="8" {{#if (eq system.psychicCosts.throwobject.costResistance 8)}}checked{{/if}}>
