    </tr>
    <tr class="throwobject">
      <td class="align-left">
        <label
          for="system.psychic.throwobject"
          class="resource-label flexlarge align-left hide-show-psychic"
          data-label="throwobject"
          data-psychic="psychic"
        >
          {{localize (lookup @root.config.psychic 'throwobject')}}
          <i id="rotate-throwobject" class="fa {{system.psychic.throwobject.rotate}}"></i>
        </label>
      </td>
      <td class="align-center">
        <label>[</label>
        <label for="system.psychic.throwobject" class="resource-label flexsmall align-center" data-label="Value">{{system.psychic.throwobject.attr1}}</label>
        <label>|</label>
        <label for="system.psychic.throwobject" class="resource-label flexsmall align-center" data-label="Value">{{system.psychic.throwobject.attr2}}</label>
        <label>]</label>
        <br/>
        <label for="system.psychic.throwobject" class="resource-label flexsmall align-center" data-label="Value">Max: {{system.psychic.throwobject.maxTrainingStatus}}</label>
      </td>
      <td class="align-center">
        <select id="system.psychic.throwobject" class="training-select-psychic" data-psychic="psychic" name="throwobject">
          <option value="untrained" {{#if (eq system.psychic.throwobject.trainingStatus "untrained")}}selected{{/if}}>Untrained</option>
          <option value="apprentice" {{#if (eq system.psychic.throwobject.trainingStatus "apprentice")}}selected{{/if}}>Apprentice</option>
          <option value="professional" {{#if (eq system.psychic.throwobject.trainingStatus "professional")}}selected{{/if}}>Professional</option>
          <option value="expert" {{#if (eq system.psychic.throwobject.trainingStatus "expert")}}selected{{/if}}>Expert</option>
          <option value="master" {{#if (eq system.psychic.throwobject.trainingStatus "master")}}selected{{/if}}>Master</option>
          <option value="legendary" {{#if (eq system.psychic.throwobject.trainingStatus "legendary")}}selected{{/if}}>Legendary</option>
        </select>
      </td>
      <td class="align-center">
        <label 
          for="system.psychic.throwobject"
          class="rollable resource-label flexsmall align-center"
          data-die="{{system.psychic.throwobject.die}}"
          data-bonus="{{system.psychic.throwobject.totalBonus}}"
          data-grit="{{system.derivedAbilityPools.gritDie.die}}"
          data-roll="{{system.psychic.throwobject.die}}+{{system.psychic.throwobject.totalBonus}}+{{system.derivedAbilityPools.gritDie.die}}"
          data-label="throwobject"
          data-rolltype="psychic-power"
          data-ability="throwobject">
          <div class="tooltip">
            {{#if (eq system.psychic.throwobject.die 'd2')}}
              <i class="fa-solid fa-coin" data-ability="throwobject"></i>
            {{else}}
              <i class="fa-solid fa-dice-{{system.psychic.throwobject.die}}" data-ability="throwobject"></i>
            {{/if}}              
            <span class="die-word">{{system.psychic.throwobject.die}}{{#if system.psychic.throwobject.isNegBase}}{{else}}+{{/if}}{{system.psychic.throwobject.totalBonus}}</span>
            {{#if (eq system.derivedAbilityPools.gritDie.die 'd2')}}
              <i class="fa-solid fa-coin" data-ability="grit"></i>
            {{else}}
              <i class="fa-solid fa-dice-{{system.derivedAbilityPools.gritDie.die}}" data-ability="grit"></i>
            {{/if}}
            <span class="die-word">{{system.derivedAbilityPools.gritDie.die}}</span>
            <span class="tooltiptext">
              <span class="tooltip-label">Die:</span> {{system.psychic.throwobject.die}}<br/>
              <span class="tooltip-label">Bonus:</span> {{system.psychic.throwobject.bonusMod}}<br/>
              <span class="tooltip-label">Hindrance Mod:</span> {{system.psychic.throwobject.hindranceMod}}<br/>
              <span class="tooltip-label">Trait Mod:</span> {{system.psychic.throwobject.traitMod}}<br/>
              <span class="tooltip-label">Cybernetic Mod:</span> {{system.psychic.throwobject.cyberMod}}<br/>
              <span class="tooltip-label">Bionetic Mod:</span> {{system.psychic.throwobject.bioMod}}
            </span>
          </div>
        </label>
      </td>
      <td class="align-right">
        {{#if (eq system.psychic.throwobject.trainingStatus "legendary")}}
        {{else}}
        <!-- Tick Success 0 - u -->
