          <option value="apprentice" {{#if (eq system.psychic.confusion.trainingStatus "apprentice")}}selected{{/if}}>Apprentice</option>
          <option value="professional" {{#if (eq system.psychic.confusion.trainingStatus "professional")}}selected{{/if}}>Professional</option>
          <option value="expert" {{#if (eq system.psychic.confusion.trainingStatus "expert")}}selected{{/if}}>Expert</option>
          <option value="master" {{#if (eq system.psychic.confusion.trainingStatus "master")}}selected{{/if}}>Master</option>
          <option value="legendary" {{#if (eq system.psychic.confusion.trainingStatus "legendary")}}selected{{/if}}>Legendary</option>
        </select>
      </td>
      <td class="align-center">
        <label 
          for="system.psychic.confusion"
          class="resource-label flexsmall align-center"
          data-die="{{system.psychic.confusion.die}}"
          data-bonus="{{system.psychic.confusion.totalBonus}}"
          data-grit="{{system.derivedAbilityPools.gritDie.die}}"
          data-roll="{{system.psychic.confusion.die}}+{{system.psychic.confusion.totalBonus}}+{{system.derivedAbilityPools.gritDie.die}}"
          data-label="{{localize (lookup @root.config.psychic 'confusion')}}"
          data-rolltype="psychic-power"
          data-ability="confusion">
          <div class="tooltip">
            {{#if (eq system.psychic.confusion.die 'd2')}}
              <i class="fa-solid fa-coin" data-ability="confusion"></i>
            {{else}}
              <i class="fa-solid fa-dice-{{system.psychic.confusion.die}}" data-ability="confusion"></i>
            {{/if}}              
            <span class="die-word">{{system.psychic.confusion.die}}{{#if system.psychic.confusion.isNegBase}}{{else}}+{{/if}}{{system.psychic.confusion.totalBonus}}</span>
            {{#if (eq system.derivedAbilityPools.gritDie.die 'd2')}}
              <i class="fa-solid fa-coin" data-ability="grit"></i>
            {{else}}
              <i class="fa-solid fa-dice-{{system.derivedAbilityPools.gritDie.die}}" data-ability="grit"></i>
            {{/if}}
            <span class="die-word">{{system.derivedAbilityPools.gritDie.die}}</span>
            <span class="tooltiptext">
              <span class="tooltip-label">Die:</span> {{system.psychic.confusion.die}}<br/>
              <span class="tooltip-label">Bonus:</span> {{system.psychic.confusion.bonusMod}}<br/>
              <span class="tooltip-label">Hindrance Mod:</span> {{system.psychic.confusion.hindranceMod}}<br/>
              <span class="tooltip-label">Trait Mod:</span> {{system.psychic.confusion.traitMod}}<br/>
              <span class="tooltip-label">Cybernetic Mod:</span> {{system.psychic.confusion.cyberMod}}<br/>
              <span class="tooltip-label">Bionetic Mod:</span> {{system.psychic.confusion.bioMod}}
            </span>
          </div>
        </label>
      </td>
